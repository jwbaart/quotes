{
  "name": "quotes",
  "version": "0.0.0",
  "scripts": {
    "firebase": "firebase",
    "cypress": "cypress",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer ./dist/quotes/stats-es2015.json",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "lint": "ng lint",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "test": "yarn build:testConfig && cypress run",
    "test:open": "yarn build:testConfig && cypress open",
    "test:stage": "yarn test -- --env envName=stage",
    "test:open:stage": "yarn test:open -- --env envName=stage",
    "deploy:firebase:rules": "firebase deploy --only firestore:rules"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "tslint --project tsconfig.json --fix",
      "pretty-quick",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/fire": "^5.2.3",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "firebase": "^7.5.2",
    "lodash.clonedeep": "^4.5.0",
    "ng-inline-svg": "^9.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^3.8.1",
    "cypress-firebase": "^0.6.0",
    "firebase-admin": "^8.9.2",
    "firebase-tools": "^7.11.0",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
